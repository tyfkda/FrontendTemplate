    "jest": "~29.3.1",
    "jest-cli": "~29.3.1",
    "sass": "~1.56.1",
    "ts-jest": "~29.0.3",
    "ts-loader": "~9.2.8",
    "typescript": "~4.6.3",
    "webpack": "~5.72.0",
    "webpack-stream": "~7.0.0"
