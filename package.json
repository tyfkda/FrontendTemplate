    "@typescript-eslint/parser": "~5.20.0",
    "browser-sync": "~2.27.9",
    "clone": "~2.1.2",
    "eslint": "~8.13.0",
    "gulp": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-ejs": "~5.1.0",
    "gulp-eslint": "~6.0.0",
